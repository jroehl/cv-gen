{
  "name": "cv",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "gen:schema": "npx typescript-json-schema --noExtraProps --skipLibCheck --tsNodeRegister --esModuleInterop --required src/types.ts SchemaCV --out cv.schema.json",
    "build:pdf": "npx tsx bin/cv-gen.tsx public/cv-johannemrich.json",
    "build:convert": "bin/landing-page.sh public/cv-johannemrich.json",
    "preview": "vite preview",
    "prepare": "husky install"
  },
  "dependencies": {
    "@monaco-editor/react": "^4.5.2",
    "@react-pdf/renderer": "^3.1.12",
    "ajv": "^8.12.0",
    "contentful": "^10.5.1",
    "date-fns": "^2.30.0",
    "gravatar": "^1.8.2",
    "husky": "^8.0.3",
    "lodash": "^4.17.21",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.15.0"
  },
  "devDependencies": {
    "@types/gravatar": "^1.8.3",
    "@types/lodash": "^4.14.197",
    "@types/node": "^20.5.6",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitejs/plugin-react": "^4.0.3",
    "eslint": "^8.45.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "husky": "^8.0.0",
    "tsx": "^3.12.7",
    "typescript": "^5.0.2",
    "typescript-json-schema": "^0.60.0",
    "vite": "^4.4.5"
  }
}
